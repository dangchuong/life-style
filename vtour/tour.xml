        <layer name="spot1" style="tooltip" tooltip="エキサイティングシート" scale="3" url="skin/point.png" align="lefttop" edge="center" x="765" y="680"  zorder="2" onclick="loadscene(scene_1, null, MERGE, BLEND(1));" />
